package com.web.cafe.dto;

public class MenuDto {

	private int mno;
	private int cafeno;
	private String item;
	private String updateItem;
	private int price;
	public int getMno() {
		return mno;
	}
	public void setMno(int mno) {
		this.mno = mno;
	}
	public int getCafeno() {
		return cafeno;
	}
	public void setCafeno(int cafeno) {
		this.cafeno = cafeno;
	}
	public String getItem() {
		return item;
	}
	public void setItem(String item) {
		this.item = item;
	}
	public String getUpdateItem() {
		return updateItem;
	}
	public void setUpdateItem(String updateItem) {
		this.updateItem = updateItem;
	}
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	public MenuDto(int mno, int cafeno, String item, String updateItem, int price) {
		super();
		this.mno = mno;
		this.cafeno = cafeno;
		this.item = item;
		this.updateItem = updateItem;
		this.price = price;
	}
	public MenuDto() {
		super();
		// TODO Auto-generated constructor stub
	}
	@Override
	public String toString() {
		return "MenuDto [mno=" + mno + ", cafeno=" + cafeno + ", item=" + item + ", updateItem=" + updateItem
				+ ", price=" + price + "]";
	}
	
	
}
